{
  "name": "typecomposer-template",
  "description": "A language support extension for TypeComposer",
  "author": "zico15",
  "license": "MIT",
  "version": "0.0.17",
  "repository": {
    "type": "git",
    "url": "https://github.com/TypeComposer/language-service/tree/main/vscode-language-service"
  },
  "publisher": "typecomposer-language-support",
  "categories": [],
  "keywords": [
    "multi-root ready"
  ],
  "engines": {
    "vscode": "^1.100.0"
  },
  "main": "./client/out/extension",
  "icon": "images/icon.png",
  "contributes": {
    "languages": [
      {
        "id": "tsx-template",
        "aliases": [
          "TSX Template",
          "tsx-template"
        ],
        "extensions": [
          ".template"
        ],
        "configuration": "./language/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tsx-template",
        "scopeName": "source.tsx-template",
        "path": "./syntaxes/tsx.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:tsx-template"
  ],
  "scripts": {
    "package": "vsce package",
    "build": "tsc -b",
    "watch": "tsc -b -w",
    "lint": "eslint",
    "postinstall": "cd client && npm install && cd ..",
    "test": "sh ./scripts/e2e.sh"
  },
  "dependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "glob": "^11.0.0",
    "recast": "^0.23.11",
    "typescript": "^5.9.2",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22",
    "eslint": "^9.13.0",
    "mocha": "^10.3.0",
    "typescript-eslint": "^8.39.0",
    "vsce": "^2.15.0"
  }
}
